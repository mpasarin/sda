  "homepage": "https://github.com/mpasarin/sda#readme",
  "devDependencies": {
    "@types/node": "^8.10.39",
    "electron": "^7.1.10",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "webpack": "~4.41.5",
    "webpack-cli": "~3.3.10",
    "html-webpack-plugin": "~3.2.0",
    "ts-loader": "~6.2.1",
    "react": "~16.12.0",
    "react-dom": "~16.12.0",
    "@types/react": "~16.9.19",
